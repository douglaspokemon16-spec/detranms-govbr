{
  "version": 2,
  "builds": [
    {
      "src": "api/consultar.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/consultar",
      "dest": "/api/consultar",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "https://detranmatogrossosul-govbr.vercel.app/assets/$1"
    },
    {
      "src": "/_next/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "https://detranmatogrossosul-govbr.vercel.app/_next/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
